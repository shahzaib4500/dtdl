{
  "name": "dtdl-ai-challenge",
  "version": "1.0.0",
  "description": "AI-driven digital twin platform for mine sites using DTDL and telemetry",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reload-client-data": "tsx scripts/reload-client-data.ts",
    "db:update-timestamps": "tsx scripts/update-telemetry-timestamps.ts",
    "db:check-timestamps": "tsx scripts/check-telemetry-timestamps.ts",
    "db:verify": "tsx scripts/verify-setup.ts",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "dtdl",
    "digital-twin",
    "ai",
    "langchain",
    "mining"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@prisma/client": "^5.22.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "@types/papaparse": "^5.3.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "commander": "^12.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "papaparse": "^5.4.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "prisma": "^5.19.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  }
}
